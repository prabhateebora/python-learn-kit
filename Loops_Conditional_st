
a=[[1,2, 6], [3,4], [1, 8,9, 8], [8,9], [6,3]]
sum=0
for val in a:
    max = val[0]
    for inner in val:
        if inner > max:
            max = inner
    sum = sum + max
    
    
print('the sum is',sum)





n=range(1, 51, 2)
for i in n:
    print(i)




l = ['p' ,'q']
a=[]

for i in range(1,6):
    for j in l:
        a.append(j+str(i))
print(a)




n=input("enter the value")
x=int(n)
i = 0
while x > i:
  print(x)
  x = x - 1
